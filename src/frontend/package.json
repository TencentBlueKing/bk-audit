    "src/**/*.(vue|js|jsx|ts|tsx)": [
      "eslint . --ext .vue,.js,.jsx,.ts,.tsx --fix"
    ],
    "src/**/*.(vue|css)": [
      "stylelint --fix"
    ]
  },
  "engines": {
    "node": ">= 16.16.0",
    "npm": "6.14.15"
  }
}
